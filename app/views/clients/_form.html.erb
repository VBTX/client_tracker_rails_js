<%= form_for(@client) do |c| %>
	<label> Client Name: </label><br>
	<%=  c.text_field :business_name %><br>
	<label> Address: </label><br>
	<%=  c.text_field :address %><br>
	<label> Website: </label><br>
	<%=  c.url_field :website %><br>
	<label> Email: </label><br>
	<%=  c.email_field :email %><br>
	<br><br>
	<%= c.label :project_id, "Select a Project" %>
  	<%= c.collection_select :project_id, Project.all, :id, :description, prompt: true %>
  	<br><br>

      <% end %>

      <br>
      <br>
	<%= c.submit %>
<%end%>